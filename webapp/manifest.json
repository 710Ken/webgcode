{
    "name": "USB Nico CNC",
    "version": "0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "permissions": ["usb", "webview"],
    "optional_permissions": [
        {
            "usbDevices": [
                {
                    "vendorId": 1155,
                    "productId": 65535
                }
            ]
        }
    ],
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "sandbox": {
        "pages": ["CAM.html"]
    }
}